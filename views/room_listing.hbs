<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>Sunset Hammock</title>
     {{!-- Bootstrap library --}}
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <!-- Library for the hamburger menu, star rating and social media icons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/all.min.css">
    {{!-- Custom CSS --}}
    <link rel="stylesheet" href="/css/room_listing_style.css">
    <link rel="stylesheet" href="/css/signup.css">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat&display=swap" rel="stylesheet"> 
    <link href="https://fonts.googleapis.com/css2?family=Molle:ital@1&display=swap" rel="stylesheet"> 
</head>
<body>

{{> header user}}

    <main>
        <div class="topBanner">
            <div class="container">
                <div class="main-heading">
                     <!-- <h1 class="title">Stays</h1> -->
                    <h2 class="subtitle">Stays</h2>
                </div>
            </div>
        </div> 

        {{> booking}}
        
        <section class="stays">
            <div class="container">
                <h5 class="section-head">
                    <span class="heading">Get closer to nature</span>
                    <span class="subheading">Enjoy the fresh air</span>
                </h5>
               
                <div class="row">
                     {{#if hasListings}} {{#each listings}}
                                         <div class="col-md-6 col-lg-4">
                        <div class="card">
                            <img src="/photos/{{filename}}" alt="" class="card-img-top image-fluid">                        
                                
                                <!-- <a href="#">
                                    <i class="far fa-heart"></i>
                                </a> -->
                            
                            
                            
                        
                            <div class="card-body">
                                <div class="row align-items-center justify-content-start">
                                <h1 class="col-9 card-title"><a href="./details_page">{{listingName}}</a></h1>
                                <div class="favorite">
                                    <a href="#"><i class="col-3 far fa-heart"></i></a>
                                </div>
                                </div>
                                 <a class="city-link" href="/room_listing"> <span class="subheading">{{location}} </span></a>
                                <br>
                                <br>
                                <span class="card-text">${{price}} <span class="per-night">Per night</span>
                                </span>
                                <div class="place-rating">
                                    <i class="fas fa-star rating"></i>
                                    <i class="fas fa-star rating"></i>
                                    <i class="fas fa-star rating"></i>
                                    <i class="fas fa-star rating"></i>
                                    <i class="fas fa-star-half-alt rating"></i>
                                </div>
                                <p class="card-text">
                                    {{details}}
                                </p>
                                <!-- <a href="#" class="btn stays-btn">Book now &rarr;</a> -->
                            </div>
                        </div>
                    </div>
                    {{/each}}{{/if}}
                     {{#if hasLocation}} {{#each locations}}
                    <div class="col-md-6 col-lg-4">
                        <div class="card">
                            <img src="/photos/{{filename}}" alt="" class="card-img-top image-fluid">                        
                                
                                <!-- <a href="#">
                                    <i class="far fa-heart"></i>
                                </a> -->
                            
                            
                            
                        
                            <div class="card-body">
                                <div class="row align-items-center justify-content-start">
                                <h1 class="col-9 card-title"><a href="./details_page">{{listingName}}</a></h1>
                                <div class="favorite">
                                    <a href="#"><i class="col-3 far fa-heart"></i></a>
                                </div>
                                </div>
                                 <a class="city-link" href="/room_listing"> <span class="subheading">{{location}} </span></a>
                                <br>
                                <br>
                                <span class="card-text">${{price}} <span class="per-night">Per night</span>
                                </span>
                                <div class="place-rating">
                                    <i class="fas fa-star rating"></i>
                                    <i class="fas fa-star rating"></i>
                                    <i class="fas fa-star rating"></i>
                                    <i class="fas fa-star rating"></i>
                                    <i class="fas fa-star-half-alt rating"></i>
                                </div>
                                <p class="card-text">
                                    {{details}}
                                </p>
                                <!-- <a href="#" class="btn stays-btn">Book now &rarr;</a> -->
                            </div>
                        </div>
                    </div>
                    
                  {{/each}}{{/if}}
     
            </div>
        </section>

      


       

    </main>

{{> footer}}

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.min.js" integrity="sha384-w1Q4orYjBQndcko6MimVbzY0tgp4pWB4lZ7lr30WKz0vr/aWKhXdBNmNb5D92v7s" crossorigin="anonymous"></script>
    <script src="/javascript/script.js"></script>
</body>
</html>